<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_std</servlet-name>
        <servlet-class>add_std</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stdid</servlet-name>
        <servlet-class>stdid</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>get_det</servlet-name>
        <servlet-class>get_det</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>up_det</servlet-name>
        <servlet-class>up_det</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_user</servlet-name>
        <servlet-class>add_user</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_std</servlet-name>
        <url-pattern>/add_std</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stdid</servlet-name>
        <url-pattern>/stdid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>get_det</servlet-name>
        <url-pattern>/get_det</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>up_det</servlet-name>
        <url-pattern>/up_det</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_user</servlet-name>
        <url-pattern>/add_user</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
